{
    "name": "nezhelskoy/yii2-highlight",
    "description": "Yii2 highlight.js extension",
    "type": "yii2-extension",
    "keywords": ["yii2", "highlight", "highlight.js"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dmitry A. Nezhelskoy",
            "email": "dmitry@nezhelskoy.pro",
            "homepage": "http://nezhelskoy.pro"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2"
    },
    "autoload": {
            "psr-4": {
                    "nezhelskoy\\highlight\\": "src/",
                    "nezhelskoy\\highlight\\examples\\": "examples/"
            }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
